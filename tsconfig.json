{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedLocals": true,
    "sourceMap": false,
    "declaration": false,
    "pretty": true,
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules"
  ]
}
